{
    "version": 2,
    "headers": [
        {
            "source": "/_next/static/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        },
        {
            "source": "/models/(.*.glb)",
            "headers": [
                {
                    "key": "Content-Type",
                    "value": "model/gltf-binary"
                }
            ]
        }
    ]
}